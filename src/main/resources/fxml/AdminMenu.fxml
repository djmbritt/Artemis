<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" fillWidth="false" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sbnvw.artemis.controllers.AdminMenuController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Button minHeight="25.0" minWidth="150.0" mnemonicParsing="false" onAction="#makeNewPage" text="Animal Creator" />
      <Button mnemonicParsing="false" onAction="#makeNewTrait" text="Trait Creator" />
      <Button mnemonicParsing="false" onAction="#makeNewQuiz" text="Quiz Creator" />
   </children>
</VBox>
